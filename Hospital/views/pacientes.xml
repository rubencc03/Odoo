<odoo>
  <data>

    <record model="ir.actions.act_window" id="actionPacientes">
      <!-- -->
      <field name="name">Pacientes</field>
      <field name="res_model">pacientes</field>
      <field name="view_mode">tree,form</field>
    </record>
    <menuitem name="Pacientes" id="pacientes_menu" parent="diagnosticos_menu" action="actionPacientes"/>

    <record model="ir.ui.view" id="lista_pacientes_tree">
      <field name="name">Lista de Pacientes</field>
      <field name="model">pacientes</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="apellido"/>
          <field name="dni"/>
          <field name="sintomas"/>
        </tree>
      </field>
    </record>

   
   

   

  </data>
</odoo>